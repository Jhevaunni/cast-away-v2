<h1>Create User</h1>
<%= form_for :user, url: user_index_url  do |f| %>
    <p>
        <%= f.label :username %><br>
        <%= f.text_field :username %>
    </p>
    
    <p>
        <%= f.label :password %><br>
        <%= f.text_field :password %>
    </p>
    
    <p>
          <%= f.submit :"Create User" %>
    </p>
    
    <% end %>
    
    <li><%= link_to 'Back to home page', home_index_url %></li>
    
    

